{
  "version": 2,
  "name": "markwasmuth",
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.jpg",
      "use": "@vercel/static"
    },
    {
      "src": "*.pdf",
      "use": "@vercel/static"
    },
    {
      "src": "*.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/coaching", "dest": "/coaching.html" },
    { "src": "/book", "dest": "/book.html" },
    { "src": "/snaptosell", "dest": "/snaptosell.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
